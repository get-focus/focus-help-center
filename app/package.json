{
    "name": "focus-help-center",
    "version": "0.0.1",
    "description": "Help Center extension for focus",
    "scripts": {
        "build": "webpack --config ./webpack.back.js && webpack --config ./webpack.ext.js",
        "start": "better-npm-run start",
        "test": "better-npm-run test",
        "test:watch": "better-npm-run test-watch",
        "watch": "webpack --watch"
    },
    "betterScripts": {
        "start": {
            "command": "node ./dev-server",
            "env": {
                "DEV_SERVER_PORT": 9999,
                "DEV_SERVER_HOST": "localhost"
            }
        },
        "test": {
            "command": "mocha src/**/__tests__/**/*.js",
            "env": {
                "BABEL_ENV": "production"
            }
        },
        "test-watch": {
            "command": "mocha src/**/__tests__/**/*.js -w",
            "env": {
                "BABEL_ENV": "production"
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/get-focus/focus-help-center.git"
    },
    "keywords": [
        "react",
        "redux",
        "help",
        "center",
        "focus"
    ],
    "author": "KleeGroup",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/get-focus/focus-help-center/issues"
    },
    "homepage": "https://github.com/get-focus/focus-help-center/app/#readme",
    "dependencies": {
        "lodash": "4.13.1",
        "react": "15.1.0",
        "react-addons-test-utils": "15.1.0",
        "react-dom": "15.1.0",
        "react-redux": "4.4.5",
        "redux": "3.5.2",
        "redux-thunk": "2.1.0"
    },
    "devDependencies": {
        "babel-core": "6.9.1",
        "babel-loader": "6.2.4",
        "babel-preset-focus": "0.5.2",
        "better-npm-run": "0.0.8",
        "chai": "3.5.0",
        "chai-subset": "1.0.1",
        "html-webpack-plugin": "2.19.0",
        "jsdom": "9.2.1",
        "mocha": "2.5.3",
        "react-hot-loader": "1.3.0",
        "sinon": "1.16.1",
        "sinon-chai": "2.8.0",
        "webpack": "1.13.1",
        "webpack-dev-server": "1.14.1",
        "webpack-focus": "0.11.5"
    }
}

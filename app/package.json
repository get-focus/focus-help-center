{
  "name": "focus-help-center",
  "version": "0.0.1",
  "description": "Help Center extension for focus",
  "scripts": {
    "build": "better-npm-run build",
    "build:watch": "better-npm-run build --watch",
    "postinstall": "typings install",
    "start": "better-npm-run start",
    "start:watch": "better-npm-run start --watch",
    "start:extension": "better-npm-run start-extension",
    "test": "better-npm-run test",
    "test:watch": "better-npm-run test --watch",
    "watch": "webpack --watch"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --config ./webpack.back.js && webpack --config ./webpack.ext.js",
      "env": {
        "BABEL_ENV": "production",
        "NODE_ENV": "production",
        "LANG": "fr"
      }
    },
    "start": {
      "command": "node ./dev-server",
      "env": {
        "DEV_SERVER_PORT": 9999,
        "DEV_SERVER_HOST": "localhost",
        "BABEL_ENV": "development",
        "LANG": "fr",
        "APP_LAUNCHED": "back-office",
        "ENV": "production"
      }
    },
    "start-extension": {
      "command": "node ./dev-server",
      "env": {
        "DEV_SERVER_PORT": 1234,
        "DEV_SERVER_HOST": "localhost",
        "BABEL_ENV": "development",
        "LANG": "fr",
        "APP_LAUNCHED": "extension",
        "ENV": "production"
      }
    },
    "test": {
      "command": "mocha src/**/__tests__/**/*.ts*",
      "env": {
        "BABEL_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/get-focus/focus-help-center.git"
  },
  "keywords": [
    "react",
    "redux",
    "help",
    "center",
    "focus"
  ],
  "author": "KleeGroup",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/get-focus/focus-help-center/issues"
  },
  "homepage": "https://github.com/get-focus/focus-help-center/app/#readme",
  "dependencies": {
    "babel-polyfill": "6.9.1",
    "i18next": "3.3.1",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.13.1",
    "material-ui": "0.15.1",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-redux": "4.4.5",
    "react-router": "2.5.1",
    "react-simple-markdown-editor": "1.1.0",
    "react-tap-event-plugin": "1.0.0",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0",
    "remarkable": "1.6.2"
  },
  "devDependencies": {
    "awesome-typescript-loader": "1.1.1",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-preset-focus": "0.6.1",
    "babel-preset-node6": "11.0.0",
    "better-npm-run": "0.0.9",
    "chai": "3.5.0",
    "chai-subset": "1.2.2",
    "css-loader": "0.23.1",
    "enzyme": "2.3.0",
    "html-webpack-plugin": "2.21.0",
    "jsdom": "9.2.1",
    "mocha": "2.5.3",
    "node-sass": "3.8.0",
    "react-addons-test-utils": "15.1.0",
    "react-hot-loader": "1.3.0",
    "redux-actions-assertions": "1.1.0",
    "sass-loader": "3.2.1",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "ts-node": "0.9.3",
    "typescript": "1.8.10",
    "typings": "1.3.0",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1",
    "webpack-focus": "0.12.2"
  }
}
{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "globals": {
        "__dirname": false
    },
    "parser": "babel-eslint",
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-multi-spaces": 2,
        "no-multiple-empty-lines": 2,
        "no-unexpected-multiline": 2,
        "array-bracket-spacing": 2,
        "comma-spacing": 2,
        "block-spacing": 2,
        "key-spacing": 2,
        "object-curly-spacing": 2,
        "semi-spacing": 2,
        "computed-property-spacing": 2,
        "arrow-spacing": 2,
        "keyword-spacing": 2,
        "no-console": 0,
        "eol-last": 2,
        "indent": [
            "error",
            4
        ],
        "quotes": [
            "error",
            "single"
        ],
        "react/prop-types": 0,
        "react/react-in-jsx-scope": 0,
        "semi": [
            "error",
            "always"
        ]
    }
}